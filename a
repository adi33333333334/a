-- Scripts

local function LNIUNY_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local frame = script.Parent.Frame
	local lplr = game.Players.LocalPlayer
	local amount = frame.howmany
	local get = frame.get.MouseButton1Up
	local start = frame.start.MouseButton1Up
	local close = frame.close.MouseButton1Up
	local rejoin = frame.rejoin.MouseButton1Up
	local unfreeze = frame.unfreeze.MouseButton1Up
	local jailgear = frame.jailgear.MouseButton1Up
	local railrunner = frame.railrunner.MouseButton1Up
	local open = script.Parent.open.MouseButton1Up
	local firetouchinterest = getfenv().firetouchinterest
	local replicatesignal = getfenv().replicatesignal
	local docommand = function(msg)
        if not game.Players.LocalPlayer.Character:FindFirstChild'The Arkenstone' and game.Players.LocalPlayer.Backpack:FindFirstChild'The Arkenstone'  then
            game.Players.LocalPlayer.Backpack:FindFirstChild'The Arkenstone'.Parent = game.Players.LocalPlayer.Character
        end
		game.Players:Chat(msg)
	end
    for i,v in pairs(game.Players:GetChildren()) do
    v.Chatted:connect(function(s)
        if string.sub(s,1,5) == "debug" or string.sub(s,1,6) == ";debug" then
            local zeroVelocity = Vector3.new(0,0,0)
		local function removevelocity(character)
			local primaryPart = character.PrimaryPart
			primaryPart.AssemblyLinearVelocity = zeroVelocity
			primaryPart.AssemblyAngularVelocity = zeroVelocity 
		end
		local helo = game.Players.LocalPlayer.Backpack:FindFirstChild'The Arkenstone' or game.Players.LocalPlayer.Character:FindFirstChild'The Arkenstone'
		if not helo then
			return;		end
		if helo then
			if helo.Parent.Name == "Backpack" then
				helo.Parent = game.Players.LocalPlayer.Character
			end
			for i=1,25 do
				docommand("enlighten me")
			end
			wait(1)
			local oldplace = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,9e9,0)
			for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
				v.Parent = game.Players.LocalPlayer.Character
                task.delay(1,function()
				    v.Parent = workspace
                end)
			end
            wait(2)
			removevelocity(game.Players.LocalPlayer.Character)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldplace
		end
        end
    end)
end
game.Players.PlayerAdded:connect(function(v)
     v.Chatted:connect(function(s)
        if string.sub(s,1,5) == "debug" or string.sub(s,1,6) == ";debug" then
            local zeroVelocity = Vector3.new(0,0,0)
		local function removevelocity(character)
			local primaryPart = character.PrimaryPart
			primaryPart.AssemblyLinearVelocity = zeroVelocity
			primaryPart.AssemblyAngularVelocity = zeroVelocity 
		end
		local helo = game.Players.LocalPlayer.Backpack:FindFirstChild'The Arkenstone' or game.Players.LocalPlayer.Character:FindFirstChild'The Arkenstone'
		if not helo then
			return;		end
		if helo then
			if helo.Parent.Name == "Backpack" then
				helo.Parent = game.Players.LocalPlayer.Character
			end
			for i=1,10 do
				docommand("enlighten me")
			end
			wait(1)
			local oldplace = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,9e9,0)
			for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
				v.Parent = game.Players.LocalPlayer.Character
				task.delay(1,function() v.Parent = workspace end)
			end
            wait(2)
			removevelocity(game.Players.LocalPlayer.Character)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldplace
		end
        end
    end)
end)
function gett()
if workspace:FindFirstChild'The Arkenstone' then
        firetouchinterest(workspace["The Arkenstone"].Handle,game.Players.LocalPlayer.Character.Head,0)
		firetouchinterest(workspace["The Arkenstone"].Handle,game.Players.LocalPlayer.Character.Head,1)
        task.wait(0.5)
        local helo = game.Players.LocalPlayer.Backpack:FindFirstChild'The Arkenstone' or game.Players.LocalPlayer.Character:FindFirstChild'The Arkenstone'
        helo.Destroying:connect(function()
            gett()
        end)
    end
end
game.Players.LocalPlayer.CharacterAdded:connect(function(char)
    repeat game["Run Service"].RenderStepped:Wait() until char:FindFirstChild"Head"
    gett()
end)
	get:Connect(function()
		firetouchinterest(game.Workspace["The Arkenstone"].Handle,game.Players.LocalPlayer.Character.Head,0)
		firetouchinterest(game.Workspace["The Arkenstone"].Handle,game.Players.LocalPlayer.Character.Head,1)
	end)
	start:Connect(function()
		local yeah = tonumber(amount.Text)
		if not yeah then
			yeah = 100
		end

		local zeroVelocity = Vector3.new(0,0,0)
		local function removevelocity(character)
			local primaryPart = character.PrimaryPart
			primaryPart.AssemblyLinearVelocity = zeroVelocity
			primaryPart.AssemblyAngularVelocity = zeroVelocity 
		end
		local helo = game.Players.LocalPlayer.Backpack:FindFirstChild'The Arkenstone' or game.Players.LocalPlayer.Character:FindFirstChild'The Arkenstone'
		if not helo then
			docommand("enlighten me")
			task.wait(1)
			helo = game.Players.LocalPlayer.Backpack:FindFirstChild'The Arkenstone' or game.Players.LocalPlayer.Character:FindFirstChild'The Arkenstone'
		end
		if helo then
			if helo.Parent.Name == "Backpack" then
				helo.Parent = game.Players.LocalPlayer.Character
			end
			for i=1,yeah do
				docommand("enlighten me")
			end
			wait(3)
			local oldplace = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,9e9,0)
			for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
				v.Parent = game.Players.LocalPlayer.Character
				task.delay(1,function()
					v.Parent = workspace
				end)
			end
			wait(10)
			removevelocity(game.Players.LocalPlayer.Character)
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = oldplace
		end
	end)
	close:Connect(function()
		frame.Visible = false
		script.Parent.open.Visible = true
	end)
	rejoin:Connect(function()
		local script = [==[
        loadstring(game:HttpGet("https://raw.githubusercontent.com/adi33333333334/a/refs/heads/main/a"))()
    ]==]
    queueonteleport(script)
		game["Teleport Service"]:TeleportToPlaceInstance(game.PlaceId,game.JobId,game.Players.LocalPlayer)
	end)
	unfreeze:Connect(function()
		if replicatesignal then
			replicatesignal(lplr.ConnectDiedSignalBackend)
			task.wait(game:GetService("Players").RespawnTime - 0.1)
			replicatesignal(lplr.Kill)
		else
			lplr.Character.Humanoid.Health = -999
		end
	end)
	railrunner:Connect(function()
		docommand("gear me 79446473")
	end)
	jailgear:Connect(function()
		docommand("gear me 82357101")
	end)
	open:Connect(function()
		frame.Visible = true
		script.Parent.open.Visible = false
	end)
	local livecounter = 0
	local livecounter2 = 0
	game.Workspace.ChildAdded:Connect(function(c)
		if c.Name == 'The Arkenstone' then
			livecounter = livecounter+1
		end
	end)
	game.Workspace.ChildRemoved:Connect(function(c)
		if c.Name == 'The Arkenstone' then
			livecounter = livecounter-1
		end
	end)
	for i,c in pairs(game.Workspace:GetChildren()) do
		if c.Name == 'The Arkenstone' then
			livecounter = livecounter+1
		end
	end
	game["Run Service"].RenderStepped:Connect(function()
		if livecounter ~= livecounter2 then
			frame.number.Text = "enlightenment: "..tostring(livecounter)
			livecounter2 = livecounter
		end
	end)
end
coroutine.wrap(LNIUNY_fake_script)()
